super + t
    urxvt

super + r
    sensible-browser

super + c
    chromium-browser http://gauss.pa/hg/_admin/notifications

super + s
    emacsclient -c -n -e '(switch-to-buffer nil)'

super + m
    emacsclient -c -n -e '(new-clean-deft)'

super + a
    sensible-browser 'inbox.google.com'

super + p
    dmenu_run

super + equal
    gnome-screensaver-command -l

super + comma
    emacs --daemon

super + period
    urxvt -e ipython3


# numpad launchers
KP_End
    urxvt

KP_Down
    emacsclient -c -n -e '(switch-to-buffer nil)'

KP_Next
    sensible-browser


# function keys
XF86AudioMute
    amixer -D pulse sset Master mute

XF86AudioLowerVolume
    amixer -D pulse sset Master 5%- unmute > /dev/null

XF86AudioRaiseVolume
    amixer -D pulse sset Master 5%+ unmute > /dev/null

XF86MonBrightnessUp
    xbacklight +20

XF86MonBrightnessDown
    xbacklight -20

Print
    gnome-screenshot

super + F11
    setxkbmap gb

super + F12
    setxkbmap gb colemak


# biosite
super + f
    urxvt -e psql -U boron_user boron
