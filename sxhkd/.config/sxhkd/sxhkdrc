super + t
    urxvt

super + r
    firefox

super + s
    emacsclient -c -n -e '(switch-to-buffer nil)'

super + m
    emacsclient -c -n -e '(new-clean-deft)'

super + a
    firefox -new-window 'inbox.google.com'

super + p
    dmenu_run

super + equal
    gnome-screensaver-command -l

super + comma
    emacs --daemon

super + period
    urxvt -e ipython3


# function keys
XF86AudioMute
    amixer -D pulse sset Master mute

XF86AudioLowerVolume
    amixer -D pulse sset Master 5%- unmute > /dev/null

XF86AudioRaiseVolume
    amixer -D pulse sset Master 5%+ unmute > /dev/null

XF86MonBrightnessUp
    xbacklight +20

XF86MonBrightnessDown
    xbacklight -20

Print
    gnome-screenshot

super + F11
    setxkbmap gb

super + F12
    setxkbmap gb colemak
